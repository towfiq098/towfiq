#include <stdio.h>
int main(){
    int a[10][10],n,i,j;
    int mainDiag=0,secDiag=0;
    printf("Enter order of square matrix:");
    scanf("%d",&n);
    printf("Enter elements:\n");
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<n;i++){
        mainDiag+=a[i][i];
        secDiag+=a[i][n-i-1];
    }
    printf("Main diagonal sum = %d\n",mainDiag);
    printf("Secondary diagonal sum = %d\n",secDiag);
    return 0;
}
