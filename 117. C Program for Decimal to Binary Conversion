#include<stdio.h>
int main(){
    int decimal,binary[50],i=0;
    printf("Enter a decimal number:");
    scanf("%d",&decimal);
    while(decimal!=0){
        binary[i]=decimal%2;
        decimal/=2;
        i++;
    }
    printf("Decimal to Binary:");
    for(i=i-1;i>=0;i--)
        printf("%d",binary[i]);
    printf("\n");
    return 0;
}
