#include<stdio.h>
#include<string.h>
#include<ctype.h>
int main(){
    char str[1000];
    int alphabet[26]={0},i;
    int isPangram=1;
    printf("Enter a sentence:");
    getchar(); 
    fgets(str,sizeof(str),stdin);
    for(i=0;str[i];i++){
        if(isalpha(str[i]))
            alphabet[tolower(str[i])-'a']=1;
    }
    for(i=0;i<26;i++){
        if(alphabet[i]==0){
            isPangram=0;
            break;
        }
    }
    if(isPangram)
        printf("Pangram\n");
    else
        printf("Not Pangram\n");
    return 0;
}
