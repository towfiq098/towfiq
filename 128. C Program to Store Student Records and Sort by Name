#include<stdio.h>
#include<string.h>
struct Student{
    char name[50];
    int age;
    float marks;
};
int main(){
    struct Student s[5],temp;
    int i,j;
    printf("Enter information of 5 students:\n");
    for(i=0;i<5;i++){
        printf("Name:"); scanf("%s",s[i].name);
        printf("Age:"); scanf("%d",&s[i].age);
        printf("Marks:"); scanf("%f",&s[i].marks);
    }
    for(i=0;i<4;i++){
        for(j=i+1;j<5;j++){
            if(strcmp(s[i].name,s[j].name)>0){
                temp=s[i];
                s[i]=s[j];
                s[j]=temp;
            }
        }
    }
    printf("\nStudents sorted by name:\n");
    for(i=0;i<5;i++)
        printf("%s %d %.2f\n",s[i].name,s[i].age,s[i].marks);
    return 0;
}
