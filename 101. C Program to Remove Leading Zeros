#include<stdio.h>
#include<string.h>
int main(){
    char str[100];
    int i=0;
    printf("Enter a number string:");
    scanf("%s",str);
    while(str[i]=='0'){
        i++;
    }
    if(str[i]=='\0')
        printf("0\n");
    else
        printf("After removing leading zeros: %s\n",str + i);
    return 0;
}
