#include <stdio.h>
#include <math.h>
int main() {
    int a[10][10],n,i,j;
    float normal=0,trace=0;
    printf("Enter order of square matrix:");
    scanf("%d",&n);
    printf("Enter elements:\n");
    for(i=0;i<n;i++)
        for(j=0;j<n;j++)
            scanf("%d",&a[i][j]);
    for(i=0;i<n;i++){
        trace+=a[i][i];
        for(j=0;j<n;j++)
            normal+=a[i][j]*a[i][j];
    }
    normal = sqrt(normal);
    printf("Trace = %.2f\n", trace);
    printf("Normal = %.2f\n", normal);
    return 0;
}
